pipeline {
    agent any

    stages {
        stage('clone from github ') {
            steps {
                git branch: 'main', url: 'https://github.com/madhu-1994/madhutest.git'
                echo 'Hello World'
            }
        }    
            stage('build the source code') {
            steps {
                
                echo 'Hello World'
            }
            }    
            stage('deploy the build') {
            steps {
                
                echo 'Hello World'
            }
        }
    }
}

